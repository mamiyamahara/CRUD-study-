<h1>編集画面</h1>

  <%= form_for(@list, url: "/todolists/#{@list.id}") do |f| %>
    <h4>タイトル</h4>
      <%= f.text_field :title %>
    <br>
    <h4>本文</h4>
      <%= f.text_area :body %>
    <br>
    <h4>画像</h4>
      <%= f.attachment_field :image %>
    <br><br>
      <%= f.submit '保存' %>
  <% end %>
